/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
    ],
    "baseUrl": "./",
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@search/*": [
        "src/app/search/*"
      ],
      "@registrations/*": [
        "src/app/registrations/*"
      ],
      "@config/*": [
        "src/app/config/*"
      ],
      "@calendar/*": [
        "src/app/calendar/*"
      ],
      "@models/*": [
        "src/app/shared/models/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
